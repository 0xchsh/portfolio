      "sections": [
        {
          "id": "mapper-overview",
          "title": "Overview",
          "description": "Mapper is a location-based discovery app that helps users find and save interesting places. Features include custom maps, sharing, and collaborative planning.",
          "mockups": [
            { "id": "mapper-1", "type": "desktop", "src": "/mockups/mapper-desktop-1.png", "alt": "Mapper main view" },
            { "id": "mapper-2", "type": "mobile", "src": "/mockups/mapper-mobile-1.png", "alt": "Mapper mobile" },
            { "id": "mapper-3", "type": "mobile", "src": "/mockups/mapper-mobile-2.png", "alt": "Location detail" }
          ]
        },
        {
          "id": "mapper-lists",
          "title": "Lists",
          "description": "Create themed lists of locations like 'Best Coffee Shops' or 'Weekend Hikes'. Lists can be public or private and shared with friends.",
          "mockups": [
            { "id": "mapper-4", "type": "desktop", "src": "/mockups/mapper-desktop-2.png", "alt": "List view" },
            { "id": "mapper-5", "type": "desktop", "src": "/mockups/mapper-desktop-3.png", "alt": "List creation" }
          ]
        },
        {
          "id": "mapper-collab",
          "title": "Collaboration",
          "description": "Plan trips with friends using collaborative maps. Real-time updates, comments, and voting on locations make group planning easy.",
          "mockups": [
            { "id": "mapper-6", "type": "desktop", "src": "/mockups/mapper-desktop-4.png", "alt": "Collaboration view" },
            { "id": "mapper-7", "type": "mobile", "src": "/mockups/mapper-mobile-3.png", "alt": "Mobile collab" },
            { "id": "mapper-8", "type": "mobile", "src": "/mockups/mapper-mobile-4.png", "alt": "Comments" },
            { "id": "mapper-9", "type": "mobile", "src": "/mockups/mapper-mobile-5.png", "alt": "Voting" }
          ]
        }
      ]
    },
