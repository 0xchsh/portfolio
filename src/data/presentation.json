      "sections": [
        {
          "id": "snack-overview",
          "title": "About",
          "description": "Snack is a social link curation platform that helps users discover and share curated collections of links. The design focuses on clean typography and intuitive navigation.",
          "mockups": [
            { "id": "snack-1", "type": "desktop", "src": "/mockups/snack-desktop-1.png", "alt": "Snack homepage" }
          ]
        },
        {
          "id": "snack-collections",
          "title": "Overview",
          "description": "Users can create themed collections with custom covers and descriptions. Each collection supports drag-and-drop reordering and inline editing.",
          "mockups": [
            { "id": "snack-2", "type": "frame", "src": "", "alt": "Mobile collection" }
          ]
        },
        {
          "id": "snack-discovery",
          "title": "Discovery",
          "description": "The discovery feed surfaces trending collections and follows. Personalized recommendations based on user interests and engagement patterns.",
          "mockups": [
            { "id": "snack-6", "type": "desktop", "src": "/mockups/snack-desktop-4.png", "alt": "Discovery feed" }
          ]
        },
        {
          "id": "snack-profile",
          "title": "Profile",
          "description": "User profiles showcase their collections and activity. Stats include total links saved, collections created, and follower count.",
          "mockups": [
            { "id": "snack-7", "type": "mobile", "src": "/mockups/snack-mobile-3.png", "alt": "Profile mobile" },
            { "id": "snack-8", "type": "mobile", "src": "/mockups/snack-mobile-4.png", "alt": "Profile settings" }
          ]
        }
      ]
    },
