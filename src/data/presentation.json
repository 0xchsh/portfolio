      "sections": [
        {
          "id": "synth-overview",
          "title": "Overview",
          "description": "Synth is a music creation tool that lets anyone make beats and melodies. The interface is designed to be approachable for beginners while powerful enough for pros.",
          "mockups": [
            { "id": "synth-1", "type": "desktop", "src": "/mockups/synth-desktop-1.png", "alt": "Synth main interface" }
          ]
        },
        {
          "id": "synth-sequencer",
          "title": "Sequencer",
          "description": "The step sequencer allows pattern creation with drag-and-drop ease. Supports multiple instruments, tempo control, and pattern chaining.",
          "mockups": [
            { "id": "synth-2", "type": "desktop", "src": "/mockups/synth-desktop-2.png", "alt": "Sequencer view" },
            { "id": "synth-3", "type": "desktop", "src": "/mockups/synth-desktop-3.png", "alt": "Pattern editor" },
            { "id": "synth-4", "type": "mobile", "src": "/mockups/synth-mobile-1.png", "alt": "Mobile sequencer" }
          ]
        },
        {
          "id": "synth-instruments",
          "title": "Instruments",
          "description": "A library of synthesizers and drum machines with customizable parameters. Each instrument has a unique visual identity and sound profile.",
          "mockups": [
            { "id": "synth-5", "type": "desktop", "src": "/mockups/synth-desktop-4.png", "alt": "Instrument browser" },
            { "id": "synth-6", "type": "desktop", "src": "/mockups/synth-desktop-5.png", "alt": "Synth controls" }
          ]
        },
        {
          "id": "synth-export",
          "title": "Export",
          "description": "Export creations in various formats including WAV, MP3, and MIDI. Direct sharing to social platforms and collaboration features.",
          "mockups": [
            { "id": "synth-7", "type": "mobile", "src": "/mockups/synth-mobile-2.png", "alt": "Export options" },
            { "id": "synth-8", "type": "mobile", "src": "/mockups/synth-mobile-3.png", "alt": "Share screen" },
            { "id": "synth-9", "type": "mobile", "src": "/mockups/synth-mobile-4.png", "alt": "Sharing complete" },
            { "id": "synth-10", "type": "mobile", "src": "/mockups/synth-mobile-5.png", "alt": "Social preview" }
          ]
        }
      ]
    }
